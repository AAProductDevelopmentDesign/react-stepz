{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "sourceMap": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "noEmit": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "lib": ["es6", "dom"]
  },
  "exclude": ["node_modules", "dist"]
}
